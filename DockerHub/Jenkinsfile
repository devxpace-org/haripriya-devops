pipeline {
    agent any

    stages {
        stage('Build and Deploy') {
            steps {
                sh '''
                    chmod 666 /var/run/docker.sock
                    docker build -t hrthummala/devxpace:nicejob-${version} DockerHub
                    docker push hrthummala/devxpace:nicejob-${version}
                   '''          
            }
         }
    }
}
