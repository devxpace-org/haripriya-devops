pipeline {
    agent any

    stages {
        stage('Build and Deploy') {
            steps {
                sh '''
                    sudo docker build -t hrthummala/devxpace:haripriya-${version} DockerHub
                    sudo docker push hrthummala/devxpace:haripriya-${version}
                    chmod 666 /var/run/docker.sock
                   '''          
            }
         }
    }
}

